{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for building React single-page application",
  "main": "/src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --no-cache --watch",
    "build": "cross-env NODE_ENV=production parcel build src/index.html",
    "start": "cross-env NODE_ENV=development parcel serve src/index.html -p 3000"
  },
  "author": "Jeremy Lin",
  "license": "MIT",
  "dependencies": {
    "history": "^4.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  }
}
